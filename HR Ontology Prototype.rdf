    <!-- http://www.semanticweb.org/phiba/ontologies/2024/0/HR-Ontology-Prototype#Nonpermanent_Appointment_Action_Without_Time_Limitation -->

    <owl:Class rdf:about="http://www.semanticweb.org/phiba/ontologies/2024/0/HR-Ontology-Prototype#Nonpermanent_Appointment_Action_Without_Time_Limitation">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologyrepository.com/CommonCoreOntologies/designated_by"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/phiba/ontologies/2024/0/HR-Ontology-Prototype#Appointment_NOA_Identifier"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/pr#lacks_part"/>
                                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/phiba/ontologies/2024/0/HR-Ontology-Prototype#Not_To_Exceed_Date_Identifier"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/phiba/ontologies/2024/0/HR-Ontology-Prototype#Nonpermanent_Appointment_Action"/>
