    <!-- http://www.semanticweb.org/phiba/ontologies/2024/0/HR-Ontology-Prototype#Career-Conditional_Appointment_Identifier -->

    <owl:Class rdf:about="http://www.semanticweb.org/phiba/ontologies/2024/0/HR-Ontology-Prototype#Career-Conditional_Appointment_Identifier">
