    <!-- http://www.semanticweb.org/phiba/ontologies/2024/0/HR-Ontology-Prototype#Speech_Adjustment_Skill -->

    <owl:Class rdf:about="http://www.semanticweb.org/phiba/ontologies/2024/0/HR-Ontology-Prototype#Speech_Adjustment_Skill">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/phiba/ontologies/2024/0/HR-Ontology-Prototype#Speaking_Skill"/>
